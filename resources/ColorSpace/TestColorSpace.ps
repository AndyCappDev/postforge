% Simple CIE-based color space example
[
    /CIEBasedABC
    <<
        /WhitePoint [0.9505 1.0000 1.0890]  % D65 white point
        /BlackPoint [0.0000 0.0000 0.0000]
        
        % Range for ABC components
        /RangeABC [0.0 1.0 0.0 1.0 0.0 1.0]
        
        % Decode ABC - linear transformation
        /DecodeABC [
            { dup 0.04045 lt 
                { 12.92 div }
                { 0.055 add 1.055 div 2.4 exp }
                ifelse
            } bind
            { dup 0.04045 lt
                { 12.92 div } 
                { 0.055 add 1.055 div 2.4 exp }
                ifelse
            } bind
            { dup 0.04045 lt
                { 12.92 div }
                { 0.055 add 1.055 div 2.4 exp }
                ifelse  
            } bind
        ]
        
        % Matrix ABC to XYZ (sRGB primaries)
        /MatrixABC [
            0.4124 0.2126 0.0193
            0.3576 0.7152 0.1192
            0.1805 0.0722 0.9505
        ]
        
        % Range for LMN components  
        /RangeLMN [0.0 0.9505 0.0 1.0000 0.0 1.0890]
        
    >>
] % ColorSpace array completed

(TestColorSpace resource loaded\n) print