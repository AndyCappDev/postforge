        1 add                           % Increase the loops counter.
        2.1 9.9 add pop                 % Addition function.
        .132 .992 add pop               % Addition function.
        5.5 1.1 div pop                 % Division function.
        325.32 43.543 div pop           % Division function.
        875 5 idiv pop                  % Integer division function.
        5000 25 idiv pop                % Integer division function.
        252 3 mod pop                   % Modulo function.
        24982 15 mod pop                % Modulo function.
        4.554 5.4999 mul pop            % Multiplication function.
        .9814 .4612 mul pop             % Multiplication function.
        3.3 1.9 sub pop                 % Subtraction function.
        1.111 9 sub pop                 % Subtraction function.
        -9919.746 abs pop               % Absolut value function.
        -239.5181 abs pop               % Absolut value function.
        7343.43 neg pop                 % Negation function.
        7343.43 neg pop                 % Negation function.
        1.5673 ceiling pop              % Ceiling function.
        1.5673 ceiling pop              % Ceiling function.
        -1.2323 floor pop               % Floor function.
        -1.2323 floor pop               % Floor function.
        675.2432 round pop              % Rounding function.
        675.2432 round pop              % Rounding function.
        1.21121 truncate pop            % Truncation function.
        1.21121 truncate pop            % Truncation function.
        4.5 sqrt pop                    % Square root function.
        4.5 sqrt pop                    % Square root function.
        1 0 atan pop                    % Arc Tangeant function.
        1 0 atan pop                    % Arc Tangeant function.
        65 cos pop                      % Cosinus function.
        65 cos pop                      % Cosinus function.
        89 sin pop                      % Sinus function.
        89 sin pop                      % Sinus function.
        10 3 exp pop                    % Exponentiation function.
        10 3 exp pop                    % Exponentiation function.
        4 ln pop                        % Natural logarithmic function.
        4 ln pop                        % Natural logarithmic function.
        5 log pop                       % Base 10 logarithmic function.
        5 log pop                       % Base 10 logarithmic function.
        rand pop                        % Random function.
        rand pop                        % Random function.
        2 srand rrand pop               % More random function.
        2 srand rrand pop               % More random function.
    } loop pop                          % Do the functions again.
} def


