% --------------------------------------------------------------------
%                       Get printer name
% --------------------------------------------------------------------
/getname {
    statusdict /product known           % Check if variable exists.
    { /statusdict load                  % Load the proper dictionary.
      /product get                      % Get the product name.
    }{ (Unknown device) } ifelse        % If unknown.
    /printername exch def               % Define the variable.
} def

% --------------------------------------------------------------------
%                  Get user time in seconds.
% --------------------------------------------------------------------
/gettime {
    usertime                            % Get timer value.
    1000 div                            % Convert to seconds.
} def

% --------------------------------------------------------------------
%                  Print text centered on current point
% --------------------------------------------------------------------
/centershow {
    dup stringwidth pop                 % Find point size of string.
    2 div neg 0 rmoveto                 % Divide it by two and move to
    show                                % there. Then print it.
} def

% --------------------------------------------------------------------
%                  Print text centered on decimal place.
% --------------------------------------------------------------------
/decimalshow {
    dup dup (.) atstr dup               % Find position of decimal.
    -1 eq                               % Is there a decimal?
    { pop pop show }                    % No? Then print it all.
    { 0 exch getinterval                % Get begining of string.
    rightshow                           % Show left of decimal.
    (.) show                            % Show decimal point.
    dup length exch                     % Get length of string.
    dup (.) atstr 1 add dup             % Get position of decimal.
    4 -1 roll exch sub                  % Calc. remainder of string.
    getinterval show } ifelse           % Print the remainder.
} def

% --------------------------------------------------------------------
%                  Print text with right justification
% --------------------------------------------------------------------
/rightshow {
    dup stringwidth pop                 % Find point size of string.
    neg 0 rmoveto                       % Move to correct start point.
    show                                % Then print it.
} def

% --------------------------------------------------------------------
%            Find location of a character in a string
% --------------------------------------------------------------------
%
%   string1 string2 atstr location
%
%   string1 is the string in which to locate the character string2
%   which is of length one. Upon return, the function leaves on the
%   stack either -1 if string2 is not found in string1 or a number
%   representing the location of string2 in string1, starting with
%   0 as the first character in string1.
%
% --------------------------------------------------------------------
/atstr {
    0 get cvi                           % Convert character to integer.
    /searchchar exch def                % Define search character.
    /searchstr exch def                 % Define search string.
    /atstrcounter 0 def                 % This is where we're at.
    { atstrcounter 1 add                % Go to next position.
        searchstr length                % Are we past end of string?
        ge { /atstrcounter                  % Yes?
            -1 def exit                     % Then exit with -1
           } if                             % as the "at" code.
        searchstr atstrcounter          % If we found the
        get cvi searchchar                  % character, then
        eq { exit } if                      % exit with location.
        /atstrcounter atstrcounter      % Otherwise,
        1 add def                           % go to next location.
    } loop                              % Check another time.
    atstrcounter                        % Put result on stack.
} def

% --------------------------------------------------------------------
%                 Add two strings together.
% --------------------------------------------------------------------
%
%   string1 string2 addstr string3
%
%   string3 is an addition of string1 and string2 with string1 being
%   the first part of string3 and string2 the end of string3.
%
% --------------------------------------------------------------------
/addstr {
   /stringtwo exch def                  % Get both strings off
   /stringone exch def                      % of the stack.
   /exitstr stringone length            % Define empty string the
   stringtwo length add string def          % length of both.
   exitstr 0 stringone putinterval      % Put first string in.
   exitstr stringone length             % And then,
   stringtwo putinterval                    % put the second string.
   exitstr                              % Leave it on stack.
} def

% --------------------------------------------------------------------
%         Calculate and print difference between two times
% --------------------------------------------------------------------
/showtime {
    20 string                           % Put empty string on stack.
    cvs                                 % Convert to a string
    5 4 addzeros                        % Add lead. blanks & trail. zeros.
    remblanks                           % Remove leading blanks.
    show                                % Then print it.
} def

% --------------------------------------------------------------------
%                      Show PPST index values
% --------------------------------------------------------------------
/showppst {
    dup .0001 le                        % Is time too small?
      { pop pop                         % If so, then
        (too fast!) centershow }        % print too fast!
      { div                             % Get index value.
        .05 add                         % Round value.
        20 string cvs                   % Convert to a string.
        3 1 addzeros                    % Add zeros and spaces.
        decimalshow                     % Show the new string.
      } ifelse                          % Otherwise print it.
} def

% --------------------------------------------------------------------
%           Remove leading and trailing blanks in string
% --------------------------------------------------------------------
%
%   string1 remblanks string2
%
%   string2 is the result of removing the leading blanks and the
%   trailing blanks from string1.
%
% --------------------------------------------------------------------
/remblanks {
    {   dup 0 get cvi                   % Get first character.
        32 ne {exit} if                 % If it's a space,
        dup length 1 sub 1 exch             % then remove it
        getinterval                         % from the string.
    } loop                              % Check next one.
    {   dup dup length 1 sub get cvi    % Get last character.
        32 ne {exit} if                 % If it's a space,
        dup length 1 sub 0 exch             % then remove it
        getinterval                         % from the string.
    } loop                              % Almost done, check next.
} def

% --------------------------------------------------------------------
%         Add leading blanks and trailing zeros to string.
% --------------------------------------------------------------------
%
%   string1 leftdec rightdec addzeros string2
%
%   string2 is the result of making string1 have at least leftdec leading
%   spaces and rightdec trailing zeros.
%
% --------------------------------------------------------------------
/addzeros {
    /rightdec exch def                  % Define no. of num. bef. decimal.
    /leftdec exch def                   % Define sign. dig. after decimal.
    /totalength rightdec leftdec add    % Calculate maximum length
    1 add def                               % of string.
    dup (.) atstr                       % Find out if there is decimal.
    -1 eq { (.) addstr } if             % No? Then add decimal.
    { dup                               % Make sure we don't loose string.
        (.) atstr                       % Now where is the decimal?
        leftdec                         % Get where decimal place.
        ge { exit } if                  % Exit if in right spot.
        ( ) exch addstr                 % Add space in front of string.
    } loop                              % Check again until done.
    { dup                               % Make sure we don't loose string.
        length                          % Get length of string.
        totalength                      % Get total length of string.
        ge { exit } if                  % Exit if right length.
        (0) addstr                      % Add 0 at end of string.
    } loop                              % Do it until done.
    dup (.) atstr                       % Where is decimal at?
    rightdec add 1 add                  % What is new length of string?
    0 exch getinterval                  % Make sure string isn't too long.
} def

% --------------------------------------------------------------------
%                Calculate all the times of tests.
% --------------------------------------------------------------------
/calctimes {
    /graphtime                          % Calculate
        rambegin graphbegin                 % the graphics
    sub def                                 % time.
    /ramtime                            % Calculate
        mathbegin rambegin                  % the ram speed
    sub def                                 % time.
    /mathtime                           % Calculate
        imagebegin mathbegin                % the mathematics
    sub def                                 % time.
    /imagetime                          % Calculate
        endtime imagebegin                  % the image
    sub def                                 % time.
    /totaltime                          % Calculate
        endtime graphbegin                  % the total
    sub def                                 % time.
} def
% --------------------------------------------------------------------
%                     Draw a circle of lines
% --------------------------------------------------------------------
/circleoflines {
    save 3 1 roll                       % Save current coordinate settings.
    translate                           % Change origin point.
    /turned 0 def                       % Start at 0 degrees rotation.
    degrees neg rotate                  % Make sure first line is at 0.
    .0001 setlinewidth                  % Make the smallest lines possible.
    newpath                             % Start with a new drawing path.
    { /turned turned degrees add def    % Turn by "degrees" degrees.
        turned 360 gt { exit } if       % Check if all done.
        degrees rotate                  % Rotate coordinate system.
        0 0 moveto                      % Do a line of 1 inch from
        0 1 lineto                          % center of coordinate system.
    } loop                              % Do next line.
    stroke                              % Set ink on page.
    restore                             % Restore old coordinate settings.
} def

% --------------------------------------------------------------------
%                         Draw the main border
% --------------------------------------------------------------------
/border {
    1 setlinewidth                      % Set width of border.
    newpath                             % Start a new drawing path.
    0 0 moveto                          % Draw a box starting here
    8.5 0 lineto                            % right
    8.5 11 lineto                           % back to
    0 11 lineto                             % the first
    closepath                               % point.
    stroke                              % And print it on page.
} def

% --------------------------------------------------------------------
%                       Add the graphics to the page
% --------------------------------------------------------------------
/graphics {
    .0001 setlinewidth                  % Make sure lines are small.
    newpath                             % Start a new drawing path.
    /yjump 10 numberoflines div def     % Distance for y axis.
    /xjump 7.5 numberoflines div def    % Distance for x axis.
    /curx .5 def                        % Start of x axis.
    /cury 10.5 def                      % Start of y axis.
    { curx .5 moveto                    % Draw the lines
        .5 cury lineto                      % on the left
        /curx curx xjump add def            % and bottom
        curx 8 gt { exit } if               % of the
        /cury cury yjump sub def            % page.
    } loop                              % Keep drawing until done.
    /curx .5 def                        % Start of x axis.
    /cury .5 def                        % Start of y axis.
    { curx .5 moveto                    % Draw the lines
        8 cury lineto                       % on the right
        /curx curx xjump add def            % and bottom
        curx 8 gt { exit } if               % of the
        /cury cury yjump add def            % page.
    } loop                              % Keep drawing until done.
    /curx .5 def                        % Start of x axis.
    /cury .5 def                        % Start of y axis.
    { curx 10.5 moveto                  % Draw the lines
        .5 cury lineto                      % on the left
        /curx curx xjump add def            % and top
        curx 8 gt { exit } if               % of the
        /cury cury yjump add def            % page.
    } loop                              % Keep drawing until done.
    /curx .5 def                        % Start of x axis.
    /cury 10.5 def                      % Start of y axis.
    { curx 10.5 moveto                  % Draw the lines
        8 cury lineto                       % on the right
        /curx curx xjump add def            % and top
        curx 8 gt { exit } if               % of the
        /cury cury yjump sub def            % page.
    } loop                              % Keep drawing until done.
    stroke                              % Set ink on the page.
    /degrees 360 numberoflines          % Degrees between lines
        4 mul div def                       % for circle.
    1.7 1.7 circleoflines               % Do a circle in bottom-left.
    6.8 1.7 circleoflines               % Do another in bottom-right.
    1.7 9.3 circleoflines               % Again in top-left.
    6.8 9.3 circleoflines               % Lastly in top-right.
} def

% --------------------------------------------------------------------
%                      Do the graphics test
% --------------------------------------------------------------------
/graphtest {
    0                                   % Start the loop counter.
    { dup 10 ge {exit} if               % Exit if test done.
        1 add                           % Increase the loop counter.
        0 0 moveto 8 0 lineto           % Draw a box
        8 10.5 lineto 0 10.5 lineto     % on entire page.
        .75 setgray                     % Set color to gray.
        fill                            % Paint entire page.
        0 setgray                       % Set color back to black.
        newpath                         % Clear any current path.
        .01 setlinewidth                % Set width of lines for test.
        0 0 moveto                      % Reset current point
        5 5 2 0 45 arc                  % Make counterclockwise arc.
        0 0 moveto                      % Reset current point
        1.225 4.332 1.987 0 45 arc      % Make counterclockwise arc.
        7 7 moveto                      % Reset current point
        .1 setlinewidth                 % Set width of lines for test.
        2 2 1 90 0 arcn                 % Make clockwise arc.
        7 7 moveto                      % Reset current point
        .0987 .1122 .1564 90 0 arcn     % Make clockwise arc.
        3 8 moveto                      % Reset current point
        6 6 4 8 2 arcto                 % Append arc to.
        3 8 moveto                      % Reset current point
        1.1 2.2 3.221 6.876 2 arcto     % Append arc to.
        pop pop pop pop                 % Clear stack
        .5 setlinewidth                 % Set width of lines for test.
        pop pop pop pop                     % of arc values.
        1 3 moveto                      % Reset current point
        1 1 2 1 4 0 curveto             % Make a curve.
        1 3 moveto                      % Reset current point
        .32 setlinewidth                % Set width of lines for test.
        1.154 .913 2.1 1.99 4 0 curveto % Make a curve.
        gsave                           % Save current graphics state.
        1.897 2.345 scale               % Modify scalling.
        22 rotate                       % Rotate the coordinate system.
        5 5 translate                   % Move coordinate 0,0.
        .9878 .7656 scale               % Change scale.
        255 rotate                      % Rotate coordinates again.
        28.777 7.777 scale              % again.
        grestore                        % Restore graphics state.
        stroke                          % Draw lines along path.
        newpath                         % Clear path.
        erasepage                       % Clear the page.
    } loop pop                          % Loop until done.
} def

% --------------------------------------------------------------------
%                         Test RAM speed
% --------------------------------------------------------------------
/ramtest {
    0                                   % Start the loop counter.
    { dup 25000 ge {exit} if            % Exit if all done.
        1 add                           % Increase the loops counter.
        (this is a test string 1234567890 abcdefghijklmnopqrstuvwxyz)
        dup dup dup dup dup
        pop pop pop pop pop pop
        (Another test string)
        dup dup dup dup dup dup dup dup dup dup dup dup dup dup dup
        pop pop pop pop pop pop pop pop pop pop pop pop pop pop pop
        pop (String 3 ABCDEFGHIJKLMNOPQRSTUVWXYZ !@#$%^&*()_+)
        dup dup dup dup dup dup dup
        pop pop pop pop pop pop pop pop
    } loop pop                          % Do the functions again.
} def

