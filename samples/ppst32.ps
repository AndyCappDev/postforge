    stdppst totaltime showppst
} def

% --------------------------------------------------------------------
%                         Main program
%
%     I defined this as a function so that it is executed only once the
% whole PostScript file has been loaded into the printer. Otherwise,
% transmission to the printer would render the test un-precise. An
% example would be the difference between parallel and serial
% transmission speed, or problems with delays on a LAN spooler.
% --------------------------------------------------------------------
/speed_test {
    /graphbegin gettime def             % Get start time.
    graphtest                           % Do the graphics test
    /rambegin gettime def               % Get time for graphics.
    ramtest                             % Do RAM and font tests.
    /mathbegin gettime def              % Get time for fonts.
    mathtest                            % Do math test.
    /imagebegin gettime def             % Get time for btmapped images.
    imagetest                           % Do image processing test.
    /endtime gettime def                % Get end time.
    0 setgray                           % Set color back to black.
    border                              % Draw the border.
    graphics                            % Do the graphics around page.
    getname                             % Get the printer name.
    calctimes                           % Calculate all speed times.
    result                              % Print result of tests.

% Remove the comments (%) on the following line if you have trouble
% seeing the results because of the resolution and the size of the text.

%    2.5 2.5 scale
%    -90 rotate
%    -6.5 -4 translate

    ppstindex                           % Show PPST index results.
    showpage                            % Outpage full page.
} def
%%EndProlog
%%Page: 0 1

% --------------------------------------------------------------------
%                    Do the Processing Speed Test
% --------------------------------------------------------------------
speed_test

