<<
    /PatternType 1
    /PaintType 1  % Colored pattern
    /TilingType 1  % Constant spacing
    
    /BBox [0 0 20 20]  % Pattern cell bounding box
    /XStep 20
    /YStep 20
    
    % PaintProc - the actual pattern content
    /PaintProc {
        gsave
        
        % Draw a simple checkerboard pattern
        newpath
        0 0 moveto
        10 0 lineto
        10 10 lineto  
        0 10 lineto
        closepath
        0.8 setgray fill
        
        newpath
        10 10 moveto
        20 10 lineto
        20 20 lineto
        10 20 lineto
