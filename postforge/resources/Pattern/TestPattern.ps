        % Draw grid lines
        newpath
        0 0 moveto 20 0 lineto
        0 10 moveto 20 10 lineto
        0 20 moveto 20 20 lineto
        0 0 moveto 0 20 lineto
        10 0 moveto 10 20 lineto
        20 0 moveto 20 20 lineto
        0.2 setgray
        0.5 setlinewidth
        stroke
        
        grestore
    } bind
    
>> % Pattern dictionary completed

